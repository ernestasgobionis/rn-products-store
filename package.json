{
  "name": "ts-react-native-starter",
  "version": "0.0.1",
  "description": "Typescript React Native Starter Kit",
  "author": "Ernestas Gobionis",
  "contributors": [
    {
      "name": "Ernestas Gobionis",
      "email": "ernestasgobionis.dev@gmail.com"
    }
  ],
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:stage": "react-native run-android --variant=stageDebug --appIdSuffix=stage",
    "android:release": "react-native run-android --variant=devRelease --appIdSuffix=dev",
    "android:stage:release": "react-native run-android --variant=stageRelease --appIdSuffix=stage",
    "ios": "react-native run-ios --scheme \"ProductsShop-Dev\" --configuration Debug",
    "ios:release": "react-native run-ios --scheme \"ProductsShop-Dev\" --configuration Release",
    "start": "react-native start",
    "test:unit": "jest --testPathIgnorePatterns e2e",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --config .prettierrc.js --check src/**/**/**",
    "prettier:fix": "prettier --config .prettierrc.js --write src/**/**/**",
    "build:e2e:ios:debug": "detox build -c ios.sim.debug",
    "test:e2e:ios:debug": "detox test -c ios.sim.debug --debug-synchronization --detectOpenHandles --loglevel info --workers 2 --watch",
    "build:e2e:ios": "detox build -c ios.sim.release",
    "test:e2e:ios": "detox test -c ios.sim.release --debug-synchronization --detectOpenHandles --cleanup --record-videos failing --loglevel warn",
    "build:e2e:android": "detox build -c android.emu.release",
    "test:e2e:android": "detox test -c android.emu.release --debug-synchronization --detectOpenHandles --cleanup --loglevel verbose",
    "build:e2e:android:debug": "detox build -c android.emu.debug",
    "test:e2e:android:debug": "detox test -c android.emu.debug --debug-synchronization --detectOpenHandles --loglevel verbose --workers 2 --watch"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.9.0",
