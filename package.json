  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$J $M"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc -p ./tsconfig.json",
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  },
